{
  "name": "motia-monorepo",
  "private": true,
  "scripts": {
    "dev:node-agent": "node agents/node-agent/src/server.js",
    "dev:python-agent": "cd agents/python-agent && uvicorn src.server:app --reload --port 8000",
    "dev:agents": "pnpm run -r --parallel dev:node-agent dev:python-agent",
    "dev": "pnpm run build-config && pnpm run dev:agents & sleep 2 && pnpm run -r --parallel dev",
    "build": "pnpm run -r build",
    "build-config": "node packages/motia/src/cli/MotiaConfigBuilder.js playground",
    "test:unit": "pnpm vitest",
    "test:e2e": "pnpm playwright test",
    "test": "concurrently \"pnpm run test:unit\" \"pnpm run test:e2e\"",
    "motia": "node packages/motia/src/cli/index.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.0.0",
    "dotenv": "^16.4.7",
    "ignore": "^6.0.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.0",
    "vite": "^6.0.0",
    "vitest": "^2.1.8",
    "redis": "^4.6.11"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
