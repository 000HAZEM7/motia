{
  "messageBus": {
    "type": "redis",
    "host": "localhost",
    "port": 6379
  },
  "state": {
    "adapter": "redis",
    "host": "localhost",
    "port": 6379,
    "ttl": 3600
  },
  "endpoints": [
    {
      "name": "node-endpoint",
      "url": "http://localhost:3000",
      "runtime": "node"
    },
    {
      "name": "python-endpoint",
      "url": "http://localhost:8000",
      "runtime": "python"
    }
  ],
  "workflows": [
    {
      "name": "endpointServerHandshake",
      "components": [
        {
          "id": "api-caller",
          "endpoint": "server",
          "subscribe": [
            "handshake.callApi"
          ],
          "emits": [
            "handshake.apiResponse"
          ],
          "name": "API Caller",
          "codePath": "src/workflows/endpointServerHandshake/components/api-caller/index.js"
        },
        {
          "id": "node-finalizer",
          "endpoint": "node-endpoint",
          "subscribe": [
            "handshake.apiResponse"
          ],
          "emits": [],
          "name": "Node Finalizer",
          "codePath": "src/workflows/endpointServerHandshake/components/node-finalizer/index.js"
        },
        {
          "id": "node-starter",
          "endpoint": "node-endpoint",
          "subscribe": [
            "handshake.initiate"
          ],
          "emits": [
            "handshake.callApi"
          ],
          "name": "Node Starter",
          "codePath": "src/workflows/endpointServerHandshake/components/node-starter/index.js"
        }
      ]
    },
    {
      "name": "hybridEndpointExample",
      "components": [
        {
          "id": "analyze-data",
          "endpoint": "python-endpoint",
          "subscribe": [
            "hybrid.enriched"
          ],
          "emits": [
            "hybrid.analyzed"
          ],
          "name": "analyze-data",
          "codePath": "src/workflows/hybridEndpointExample/components/analyze-data/index.py"
        },
        {
          "id": "enrich-data",
          "endpoint": "node-endpoint",
          "subscribe": [
            "hybrid.transformed"
          ],
          "emits": [
            "hybrid.enriched"
          ],
          "name": "Enrich Data",
          "codePath": "src/workflows/hybridEndpointExample/components/enrich-data/index.js"
        },
        {
          "id": "finalize-data",
          "endpoint": "node-endpoint",
          "subscribe": [
            "hybrid.analyzed"
          ],
          "emits": [
            "hybrid.completed"
          ],
          "name": "Fanialize Data",
          "codePath": "src/workflows/hybridEndpointExample/components/finalize-data/index.js"
        },
        {
          "id": "transform-data",
          "endpoint": "python-endpoint",
          "subscribe": [
            "hybrid.validated"
          ],
          "emits": [
            "hybrid.transformed"
          ],
          "name": "Transform Data",
          "codePath": "src/workflows/hybridEndpointExample/components/transform-data/index.py"
        },
        {
          "id": "validate-data",
          "endpoint": "node-endpoint",
          "subscribe": [
            "hybrid.received"
          ],
          "emits": [
            "hybrid.validated"
          ],
          "name": "Validate Data",
          "codePath": "src/workflows/hybridEndpointExample/components/validate-data/index.js"
        }
      ]
    },
    {
      "name": "parallelMergeState",
      "components": [
        {
          "id": "join-step",
          "endpoint": "server",
          "subscribe": [
            "pms.stepA.done",
            "pms.stepB.done",
            "pms.stepC.done"
          ],
          "emits": [
            "pms.join.complete"
          ],
          "name": "join-step",
          "codePath": "src/workflows/parallelMergeState/components/join-step/index.js"
        },
        {
          "id": "start-parallel-merge",
          "endpoint": "server",
          "subscribe": [
            "pms.initialize"
          ],
          "emits": [
            "pms.start"
          ],
          "name": "Start Event",
          "codePath": "src/workflows/parallelMergeState/components/start-parallel-merge/index.js"
        },
        {
          "id": "stepA",
          "endpoint": "server",
          "subscribe": [
            "pms.start"
          ],
          "emits": [
            "pms.stepA.done"
          ],
          "name": "stepA",
          "codePath": "src/workflows/parallelMergeState/components/stepA/index.js"
        },
        {
          "id": "stepB",
          "endpoint": "server",
          "subscribe": [
            "pms.start"
          ],
          "emits": [
            "pms.stepB.done"
          ],
          "name": "stepB",
          "codePath": "src/workflows/parallelMergeState/components/stepB/index.js"
        },
        {
          "id": "stepC",
          "endpoint": "server",
          "subscribe": [
            "pms.start"
          ],
          "emits": [
            "pms.stepC.done"
          ],
          "name": "stepC",
          "codePath": "src/workflows/parallelMergeState/components/stepC/index.js"
        }
      ]
    },
    {
      "name": "wistroServerExample",
      "components": [
        {
          "id": "finalizer",
          "endpoint": "server",
          "subscribe": [
            "ws-server-example.processed"
          ],
          "emits": [],
          "name": "Finalizer",
          "codePath": "src/workflows/wistroServerExample/components/finalizer/index.js"
        },
        {
          "id": "processor",
          "endpoint": "server",
          "subscribe": [
            "ws-server-example.start"
          ],
          "emits": [
            "ws-server-example.processed"
          ],
          "name": "Processor",
          "codePath": "src/workflows/wistroServerExample/components/processor/index.js"
        },
        {
          "id": "start-server-example",
          "endpoint": "server",
          "subscribe": [
            "ws-server-example.trigger"
          ],
          "emits": [
            "ws-server-example.start"
          ],
          "name": "Start Event",
          "codePath": "src/workflows/wistroServerExample/components/start-server-example/index.js"
        }
      ]
    }
  ],
  "traffic": [
    {
      "path": "/api/hybrid-endpoint-example",
      "method": "POST",
      "type": "hybrid.received"
    },
    {
      "path": "/api/wistro-server-example",
      "method": "POST",
      "type": "ws-server-example.trigger"
    },
    {
      "path": "/api/endpoint-server-handshake",
      "method": "POST",
      "type": "handshake.initiate"
    },
    {
      "path": "/api/parallel-merge",
      "method": "POST",
      "type": "pms.initialize"
    }
  ]
}