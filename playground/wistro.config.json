{
  "wistroServer": {
    "port": 4000
  },
  "messageBus": {
    "type": "redis",
    "host": "localhost",
    "port": 6379
  },
  "agents": [
    {
      "name": "node-agent",
      "url": "http://localhost:3000",
      "runtime": "node"
    },
    {
      "name": "python-agent",
      "url": "http://localhost:8000",
      "runtime": "python"
    }
  ],
  "traffic": [
    {
      "path": "/api/hybrid-endpoint-example",
      "method": "POST",
      "type": "hybrid.received"
    },
    {
      "path": "/api/wistro-server-example",
      "method": "POST",
      "type": "ws-server-example.trigger"
    },
    {
      "path": "/api/endpoint-server-handshake",
      "method": "POST",
      "type": "handshake.initiate"
    }
  ],
  "workflows": [
    {
      "name": "endpointServerHandshake",
      "components": [
        {
          "id": "api-caller",
          "agent": "server",
          "subscribe": [
            "handshake.callApi"
          ],
          "emits": [
            "handshake.apiResponse"
          ],
          "name": "API Caller",
          "codePath": "src/workflows/endpointServerHandshake/components/api-caller/index.js"
        },
        {
          "id": "node-finalizer",
          "agent": "server",
          "subscribe": [
            "handshake.apiResponse"
          ],
          "emits": [],
          "name": "Node Finalizer",
          "codePath": "src/workflows/endpointServerHandshake/components/node-finalizer/index.js"
        },
        {
          "id": "node-starter",
          "agent": "server",
          "subscribe": [
            "handshake.initiate"
          ],
          "emits": [
            "handshake.callApi"
          ],
          "name": "Node Starter",
          "codePath": "src/workflows/endpointServerHandshake/components/node-starter/index.js"
        }
      ]
    },
    {
      "name": "hybridEndpointExample",
      "components": [
        {
          "id": "analyze-data",
          "agent": "python-agent",
          "subscribe": [
            "hybrid.enriched"
          ],
          "emits": [
            "hybrid.analyzed"
          ],
          "name": "analyze-data",
          "codePath": "src/workflows/hybridEndpointExample/components/analyze-data/index.py"
        },
        {
          "id": "enrich-data",
          "agent": "server",
          "subscribe": [
            "hybrid.transformed"
          ],
          "emits": [
            "hybrid.enriched"
          ],
          "name": "Enrich Data",
          "codePath": "src/workflows/hybridEndpointExample/components/enrich-data/index.js"
        },
        {
          "id": "finalize-data",
          "agent": "server",
          "subscribe": [
            "hybrid.analyzed"
          ],
          "emits": [
            "hybrid.completed"
          ],
          "name": "Fanialize Data",
          "codePath": "src/workflows/hybridEndpointExample/components/finalize-data/index.js"
        },
        {
          "id": "transform-data",
          "agent": "python-agent",
          "subscribe": [
            "hybrid.validated"
          ],
          "emits": [
            "hybrid.transformed"
          ],
          "name": "Transform Data",
          "codePath": "src/workflows/hybridEndpointExample/components/transform-data/index.py"
        },
        {
          "id": "validate-data",
          "agent": "server",
          "subscribe": [
            "hybrid.received"
          ],
          "emits": [
            "hybrid.validated"
          ],
          "name": "Validate Data",
          "codePath": "src/workflows/hybridEndpointExample/components/validate-data/index.js"
        }
      ]
    },
    {
      "name": "wistroServerExample",
      "components": [
        {
          "id": "finalizer",
          "agent": "server",
          "subscribe": [
            "ws-server-example.processed"
          ],
          "emits": [],
          "name": "Finalizer",
          "codePath": "src/workflows/wistroServerExample/components/finalizer/index.js"
        },
        {
          "id": "processor",
          "agent": "server",
          "subscribe": [
            "ws-server-example.start"
          ],
          "emits": [
            "ws-server-example.processed"
          ],
          "name": "Processor",
          "codePath": "src/workflows/wistroServerExample/components/processor/index.js"
        },
        {
          "id": "start-event",
          "agent": "server",
          "subscribe": [
            "ws-server-example.trigger"
          ],
          "emits": [
            "ws-server-example.start"
          ],
          "name": "Start Event",
          "codePath": "src/workflows/wistroServerExample/components/start-event/index.js"
        }
      ]
    }
  ]
}